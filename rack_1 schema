CREATE TABLE `inventory` (
  `id` int(10) NOT NULL AUTO_INCREMENT,
  `ServiceOrder` varchar(20) DEFAULT 'NONE',
  `UnitSKU` varchar(20) DEFAULT 'NONE',
  `PartType` varchar(20) DEFAULT 'NONE',
  `Remarks` varchar(255) DEFAULT NULL,
  `PartSKU` varchar(20) DEFAULT 'NONE',
  `Model` varchar(20) DEFAULT 'NONE',
  `Cart` varchar(20) DEFAULT 'NONE',
  `DateOrdered` varchar(20) DEFAULT 'NONE',
  `Status` varchar(20) DEFAULT 'AWP',
  `Checkout` varchar(20) DEFAULT 'NONE',
  PRIMARY KEY (`id`)
)
